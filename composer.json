{
    "name": "devleaks/yii2-metafizzy",
    "description": "Yii2 asset bundle wrapper around several David DeSandro javascript libraries",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","masonry", "isotope", "packery", "metafizzy", "draggabilly"],
    "license": "MIT",
    "authors": [
        {
            "name": "Pierre M",
            "email": "devleaks.be@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "bower-asset/packery": "*",
        "bower-asset/draggabilly": "*",
        "bower-asset/isotope": "*",
        "bower-asset/masonry": "*",
		"bower-asset/imagesloaded" : "*"
    },
    "autoload": {
        "psr-4": {
            "devleaks\\metafizzy\\": ""
        }
    }
}